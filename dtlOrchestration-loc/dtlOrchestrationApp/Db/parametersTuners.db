# RF Start-Up Working Position Pos0
record(ai, "DTL:MC-MCU:RFStartUpWorkingPos") {
	field(DESC, "RF Startup Working Position")
	field(VAL, "20")
	field(PINI, "YES")
	field(EGU, "mm")
	field(PREC, "4")
