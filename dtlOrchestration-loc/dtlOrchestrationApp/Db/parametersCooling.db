# RF Conditioning Working Temperature t0
record(ai, "DTL:WtrC:RFCondWorkingTmp") {
	field(DESC, "RF Cond Working Temperature")
	field(VAL, "0")
	field(PINI, "YES")
	field(EGU, "degC")
	field(PREC, "2")
}

