# LINAC operation setup:
# the actual value reflecs the code used in the state machine
record(mbbo, "DTL::Operation") {
	field(DESC, "DTL Operation Stage Cmd")
	field(VAL, "0")
	field(PINI, "YES")
	field(OMSL,	"supervisory")
	field(ZRVL,	"0")
	field(ZRST,	"INITIALIZATION")
	field(ONVL,	"101")
	field(ONST,	"STAND-BY")
	field(TWVL,	"202")
	field(TWST,	"RF CONDITIONING")
	field(THVL,	"303")
	field(THST,	"RF START-UP")
	field(FRVL,	"404")
	field(FRST,	"READY FOR OPERATION")
	field(FVVL,	"505")
	field(FVST,	"NO ACCELERATED BEAM")
	field(SXVL,	"606")
	field(SXST,	"MAINTENANCE")
}


# RF Ready used for DTL operations
