# Records for Tuners and Cooling Sub-System
#
# Condition:  | Δf1 | < | Δf2 |
#
#
# Defined by Maurizio Montis | INFN-LNL
# mail: maurizio.montis@lnl.infn.it

# RF Freq Low Threshold Δf1
record(ai, "DTL:RFS:BeamOpLDeltaFreq") {
	field(DESC, "RF Freq Low Threshold Δf1")
	field(VAL, "0")
	field(PINI, "YES")
	field(EGU, "kHz")
	field(PREC, "4")
}

# RF Freq High Threshold Δf2
record(ai, "DTL:RFS:BeamOpHDeltaFreq") {
	field(DESC, "RF Freq High Threshold Δf2")
	field(VAL, "0")
	field(PINI, "YES")
	field(EGU, "kHz")
	field(PREC, "4")
}

# RF Frequency Mode (Code)
# 909 --> Frequency Following Mode (FF Mode)
# 808 --> Fixed Frequency
#
record(ai, "DTL:RFS:FreqMode") {
	field(DESC,	"RF Frequency Mode")
	field(VAL,	"0")
	field(PINI,	"YES")
	field(EGU,	"")
	field(PREC,	"0")
}
