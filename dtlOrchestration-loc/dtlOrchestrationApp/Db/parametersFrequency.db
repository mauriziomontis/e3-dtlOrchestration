# RF Freq Low Threshold Δf1
record(ai, "DTL:RFS:BeamOpLDeltaFreq") {
	field(DESC, "RF Freq Low Threshold Δf1")
	field(VAL, "0")
	field(PINI, "YES")
	field(EGU, "kHz")
	field(PREC, "4")
}

# RF Freq High Threshold Δf2
record(ai, "DTL:RFS:BeamOpHDeltaFreq") {
	field(DESC, "RF Freq High Threshold Δf2")
	field(VAL, "0")
	field(PINI, "YES")
	field(EGU, "kHz")
	field(PREC, "4")
}
